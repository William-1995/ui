{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "baseBranches": [
      "main"
    ],
    "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchDatasources": ["npm"],
      "minimumReleaseAge": "2 days"
    },
      {
          "matchDatasources": ["cdnurl"],
          "registryUrls":["https://git.sheetjs.com/SheetJS/sheetjs"]
      }
  ],
    {
  "matchSourceUrlPrefixes": [
    "https://git.sheetjs.com/SheetJS/sheetjs/"
  ]
}
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "a.txt"
      ],
      "matchStrings": [
       "renovate: datasource=(?<datasource>.*?) depName=(?<depName>.*?)( versioning=(?<versioning>.*?))?( packageName=(?<packageName>.*?))?( registryUrl=(?<registryUrl>.*?))?( extractVersion=(?<extractVersion>.*?))?\s(ENV .*?_VERSION=|\s*?default\s*?=\s*?")(?<currentValue>.*?)"?\s"
      ]
    }
  ]
}
