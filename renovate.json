{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "baseBranches": [
      "main"
    ],
    "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchDatasources": ["npm"],
      "minimumReleaseAge": "2 days"
    },
      {
          "matchDatasources": ["cdnurl"],
          "registryUrls":["https://cdn.sheetjs.com"]
      }
  ],
  "cdnurl": {
    "fileMatch": ["https://cdn.sheetjs.com"]
  }
}
