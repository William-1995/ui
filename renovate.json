{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "baseBranches": [
      "main"
    ],
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
   {
      "matchDatasources": ["npm"],
      "minimumReleaseAge": "2 days"
   },
   {
          "matchDatasources": ["cdnurl"],
          "registryUrls":["https://git.sheetjs.com/SheetJS/sheetjs"]
    }
   ],
    "npm": {
        "fileMatch": ["package.json"],
        "enabled": true
    },
    "html": {
      "fileMatch": [
          "\\.html?$"
      ],
      "versioning": "semver",
      "digest": {
        "enabled": false
      },
      "pinDigests": false 
    }
    
}

